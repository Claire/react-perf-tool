    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.1.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "3.4.1",
    "css-loader": "^0.23.1",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "mocha": "2.3.4",
    "style-loader": "^0.13.1",
    "webpack": "1.12.9",
    "yargs": "3.32.0"
